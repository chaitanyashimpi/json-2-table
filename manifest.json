{
    "manifest_version": 3,
    "name": "JSON to Table Converter - Chiku Converter",
    "version": "2024.1.0",
    "description": "Convert JSON data to a table and download as an Excel file.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "author": "Chaitanya Shimpi"
}